type User {
  id: ID! @unique
  name: String!
  email: String!
}

type Item {
  id: ID! @unique
  title: String!
  description: String!
  image: String
  largeImage: String
  price: Int!
  createdAt: DateTime! # type exposed via prisma
  updatedAt: DateTime! # type exposed via prisma
  # user: User!
}
